{
    "name": "coreJSON",
    "version": "3.0.2",
    "description": "A parser strictly enforcing the ECMA-404 JSON standard, suitable for microcontrollers",
    "repository": {
        "type": "git",
        "url": "https://github.com/FreeRTOS/coreJSON.git"
    },
    "license": "MIT",
    "export": {
        "include": ["source/*"]
    },
    "examples": [
        {
            "name": "README",
            "base": ".",
            "files": ["README.md"]
        }
    ],
    "build": {
        "includeDir": "source/include",
        "srcDir": "source",
        "srcFilter": "+<*.c>"
    }
}
